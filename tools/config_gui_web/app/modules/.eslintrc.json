{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-console": "off",
        "camelcase": "off",
        "new-cap": "off",
        "no-await-in-loop": "off",
        "import/extensions": "off",
        "import/prefer-default-export": "off",
        "no-unused-vars": "off"
    }
}
